{
  "version": 2,
  "builds": [
    {
      "src": "server.production.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.production.js"
    },
    {
      "src": "/health",
      "dest": "/server.production.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "server.production.js": {
      "maxDuration": 30
    }
  }
}